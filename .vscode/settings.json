{
  "latex-workshop.latex.tools": [
    {
      "name": "pdflatex with TEXINPUTS",
      "command": "pdflatex",
      "args": [
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {
        "TEXINPUTS": "./template//:./common//:./include//:"
      }
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "pdflatex with TEXINPUTS",
      "tools": [
        "pdflatex with TEXINPUTS"
      ]
    }
  ]
}